<!DOCTYPE html>
<html>
<head>
    <title>{{ title }}</title>
</head>
<body>
    <h1>{{ title }}</h1>

    <div class="wrap">
        <div id="icon-generic" class="icon32"></div>
        <h2>{{ wp.__('Twig Settings') }}</h2>

        {% if user.authenticated %}
            <div class="dashboard">
                <h2>Environmental Monitoring Dashboard</h2>
                
                <div class="current-readings">
                    <div class="metric">
                        <h3>Temperature</h3>
                        <p>{{ environmental.temperature }}Â°C</p>
                    </div>

                    <div class="metric">
                        <h3>Humidity</h3>
                        <p>{{ environmental.humidity }}%</p>
                    </div>

                    <div class="metric">
                        <h3>Air Quality Index</h3>
                        <p>{{ environmental.aqi }}</p>
                        <span class="aqi-status">{{ environmental.aqi_status }}</span>
                    </div>
                </div>

                {% if user.role == 'ADMIN' %}
                    <div class="admin-controls">
                        <h3>System Settings</h3>
                        <form method="POST" action="/settings">
                            <label>Update Interval (seconds)</label>
                            <input type="number" name="interval" value="{{ settings.interval }}">
                            <button type="submit">Update</button>
                        </form>
                    </div>
                {% endif %}
            </div>
        {% endif %}

        <form method="post" action="{{ action }}">
            {% if message is defined %}
                <div class="message updated below-h2">
                    <p>{{ message }}</p>
                </div>
            {% endif %}

            {% if error is defined %}
                <div class="message error below-h2">
                    <p>{{ error }}</p>
                </div>
            {% endif %}

            <h3>{{ wp.__('Template Cache') }}</h3>
            <p>
                {{ wp.__('This wil cause all templates to be recompiled down to plain optimized code.') }}
            </p>

            {{ nonce|raw }}

            <p class="submit">
                <input type="submit" name="submit" id="submit" class="button button-primary" value="{{ wp.__('Clear Template Cache') }}">
            </p>
        </form>

        <label class="screen-reader-text" for="{{ name }}">
            {{ wp.__('Custom Template') }}
        </label>
        <select name="{{ name }}">
            <option value="">{{ wp.__('Default Template') }}</option>
            {% for file, name in templates %}
                <option value="{{ file }}"{% if value == file %} selected="selected"{% endif %}>{{ name }}</option>
            {% endfor %}
        </select>
    </div>
</body>
</html>
